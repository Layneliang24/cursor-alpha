# 数据库配置
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=alpha_db
MYSQL_USER=alpha_user
MYSQL_PASSWORD=your_secure_db_password_here

# Django配置
DJANGO_SECRET_KEY=your_very_long_and_secure_secret_key_here
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=your-domain.com,www.your-domain.com,your-server-ip

# 数据库连接
DB_NAME=alpha_db
DB_USER=alpha_user
DB_PASSWORD=your_secure_db_password_here
DB_HOST=mysql
DB_PORT=3306

# 超级用户配置
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@your-domain.com
DJANGO_SUPERUSER_PASSWORD=your_secure_admin_password_here
